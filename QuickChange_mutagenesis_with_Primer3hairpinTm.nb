Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"mutationproperresnums", "=", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"StringJoin", "[", 
       RowBox[{
        RowBox[{"StringTake", "[", 
         RowBox[{
          RowBox[{"mutation", "[", 
           RowBox[{"[", "n", "]"}], "]"}], ",", "1"}], "]"}], ",", 
        RowBox[{"ToString", "[", 
         RowBox[{"ToExpression", "[", 
          RowBox[{"StringDrop", "[", 
           RowBox[{
            RowBox[{"StringDrop", "[", 
             RowBox[{
              RowBox[{"mutation", "[", 
               RowBox[{"[", "n", "]"}], "]"}], ",", 
              RowBox[{"-", "1"}]}], "]"}], ",", "1"}], "]"}], "]"}], "]"}], 
        ",", 
        RowBox[{"StringTake", "[", 
         RowBox[{
          RowBox[{"mutation", "[", 
           RowBox[{"[", "n", "]"}], "]"}], ",", 
          RowBox[{"-", "1"}]}], "]"}]}], "]"}], ",", 
